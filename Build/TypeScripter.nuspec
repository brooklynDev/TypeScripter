<?xml version="1.0"?>
<package >
  <metadata>
    <id>TypeScripter</id>
    <version>0.5.3</version>
    <title>TypeScripter</title>
    <authors>Chris Powers</authors>
    <owners>Chris Powers</owners>
    <licenseUrl>https://raw.githubusercontent.com/cjlpowers/TypeScripter/master/LICENSE</licenseUrl>
    <projectUrl>http://cjlpowers.github.io/TypeScripter</projectUrl>
    <!--<iconUrl>http://ICON_URL_HERE_OR_DELETE_THIS_LINE</iconUrl>-->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>TypeScripter is a .NET class library used to generate TypeScript definitions for .NET types.</description>
    <releaseNotes>
      - Render 'Nullable' type as optional property.
      - Render 'System.ValueType' as 'any'.
      - Added 'WithTypeMapping' method to 'Scripter' to support custom type mappings.
    </releaseNotes>
    <copyright>Copyright 2016</copyright>
    <tags>TypeScript</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.5" />
      <group targetFramework=".NETStandard1.6">
        <dependency id="System.Runtime" version="4.1.0" />
        <dependency id="System.Resources.ResourceManager" version="4.0.0" />
        <dependency id="System.Runtime.InteropServices" version="4.1.0" />
        <dependency id="System.Diagnostics.Debug" version="4.0.10" />
        <dependency id="System.Collections" version="4.0.10" />
        <dependency id="System.Reflection" version="4.1.0" />
        <dependency id="System.Linq" version="4.1.0" />
        <dependency id="System.Runtime.Extensions" version="4.1.0" />
        <dependency id="System.IO.FileSystem" version="4.0.1" />
      </group>
    </dependencies>
  </metadata>
</package>